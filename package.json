{
  "name": "choco-ui",
  "version": "0.1.0",
  "files": ["dist"],
  "main": "./dist/choco-ui.umd.js",
  "module": "./dist/choco-ui.es.js",
  "exports": {
    ".": {
      "import": "./dist/choco-ui.es.js",
      "require": "./dist/choco-ui.es.js"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test:unit": "vitest --environment jsdom",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "storybook": "node -r esm ./node_modules/.bin/start-storybook -p 6006",
    "build-storybook": "node -r esm ./node_modules/.bin/build-storybook  -- --preview-url=/choco-ui/iframe.html"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-regular-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/vue-fontawesome": "^3.0.0-5",
    "vue": "^3.2.33"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@rushstack/eslint-patch": "^1.1.0",
    "@storybook/addon-actions": "^6.4.22",
    "@storybook/addon-essentials": "^6.4.22",
    "@storybook/addon-links": "^6.4.22",
    "@storybook/builder-vite": "^0.1.33",
    "@storybook/vue3": "^6.4.22",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "babel-loader": "^8.2.5",
    "eslint": "^8.5.0",
    "eslint-plugin-storybook": "^0.5.11",
    "eslint-plugin-vue": "^8.2.0",
    "esm": "^3.2.25",
    "jsdom": "^19.0.0",
    "prettier": "^2.5.1",
    "sass": "^1.51.0",
    "typescript": "~4.6.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.3",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^0.34.7"
  }
}
